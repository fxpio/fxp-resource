{
    "name": "fxp/resource",
    "type": "library",
    "description": "Resource management layer for doctrine",
    "keywords": ["resource", "doctrine", "batch", "manager", "validation"],
    "homepage": "https://github.com/fxpio/fxp-resource",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/dependency-injection": "^4.2.0",
        "symfony/event-dispatcher": "^4.2.0",
        "symfony/validator": "^4.2.0",
        "symfony/http-foundation": "^4.2.0",
        "symfony/expression-language": "^4.2.0",
        "symfony/translation": "^4.2.0",
        "doctrine/persistence": "^1.0.0",
        "doctrine/orm": "^2.6.0",
        "fxp/doctrine-extra": "^1.0.0",
        "fxp/doctrine-extensions": "^1.1.0"
    },
    "require-dev": {
        "doctrine/event-manager": "^1.0.0",
        "fxp/default-value": "^1.1.0",
        "symfony/config": "^4.2.0",
        "symfony/form": "^4.2.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\Resource\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
